<?xml version="1.0" encoding="utf-8"?>
<Configuration status="debug" name="demo" packages="">
    <Appenders>
        <Console name="stdout" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %p %m%n"/>
        </Console>
    </Appenders>
    <Root level="error">
        <AppenderRef ref="stdout"/>
    </Root>
</Configuration>